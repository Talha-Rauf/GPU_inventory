
<div class="form-outline">
    <input type="text" id="company" class="form-control" name="company" required>
    <label for="company" class="form-label">Company</label>
</div>

<div class="form-outline mb-4">
    <input id="model" type="text" class="form-control form-control-md" name="model" required>
    <label for="model" class="form-label">Model</label>
</div>

<div class="form-outline mb-4">
    <input id="size" type="text" class="form-control form-control-md" name="size" required>
    <label for="size" class="form-label">Size</label>
</div>

<div class="row">
    <div class="col-md-6 mb-4">
        <div class="form-outline mb-4">
            <input id="condition" type="text" class="form-control form-control-md" name="condition" required>
            <label for="condition" class="form-label">Condition</label>
        </div>
    </div>
    <div class="col-md-6 mb-4">
        <%- include('_selectUser') %>
    </div>
</div>

<div class="form-outline form-text mb-4">
    <input id="details" type="text" class="form-control form-control-md" name="details" required>
    <label for="details" class="form-label">Details</label>
</div>


