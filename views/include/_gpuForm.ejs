
<div class="form-outline mb-4">
    <input type="text" id="company" class="form-control" name="company" required>
    <label for="company" class="form-label">Company</label>
</div>

<div class="form-outline mb-4">
    <input id="model" type="text" class="form-control form-control-md" name="model" required>
    <label for="model" class="form-label">Model</label>
</div>

<div class="form-outline mb-4">
    <input id="size" type="text" class="form-control form-control-md" name="size" required>
    <label for="size" class="form-label">Size</label>
</div>

<div class="row">
    <div class="col-md-6 mb-4">
        <%- include('_selectCondition') %>
    </div>
    <div class="col-md-6 mb-4">
        <%- include('_selectUser') %>
    </div>
</div>

<div class="form-floating mb-4">
    <textarea id="details" style="height: 100px" class="form-control form-control-md" name="details" required></textarea>
    <label for="details">Details</label>
</div>


